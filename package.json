{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ignore-path .gitignore 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(ts|tsx|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "prettier --ignore-path .gitignore --list-different \"**/*.+(ts|tsx|json)\"",
    "validate": "npm run lint && npm run build"
  },
  "dependencies": {
    "@uiw/react-markdown-preview": "^4.0.5",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.3",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.1",
    "@types/react-helmet": "^6.1.5",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vite-plugin-eslint": "^1.4.0"
  }
}
