{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended","eslint-config-prettier"],
  "rules": {
    "strict": ["error", "never"],
    "no-unused-vars": [
      0,
      { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
    ]
  },
  "env": {
    "browser": true
  }
}
